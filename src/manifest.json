{
  "author": "Carlos Jeurissen",
  "homepage_url": "https://apps.jeurissen.co/",
  "developer": {
    "name": "Carlos Jeurissen",
    "url": "https://apps.jeurissen.co/"
  },
  "manifest_version": 2,
  "name": "View Page Source",
  "content_security_policy": "default-src 'none'; script-src 'self'; script-src-attr 'none'; base-uri 'none'; form-action 'none'; frame-ancestors 'self'; block-all-mixed-content; upgrade-insecure-requests; sandbox allow-same-origin allow-scripts; require-sri-for script style; report-uri https://api.jeurissen.co/reports/csp/webext/vps/",
  "version": "1.0.7",
  "permissions": [
    "activeTab"
  ],
  "applications": {
    "gecko": {
      "id": "view-page-source@apps.jeurissen.co"
    }
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "view-page-source@apps.jeurissen.co"
    }
  },
  "background": {
    "page": "background.html",
    "persistent": false
  },
  "browser_action": {
    "browser_style": false,
    "default_icon": {
      "16": "images/icon-16.png",
      "24": "images/icon-24.png",
      "32": "images/icon-32.png",
      "48": "images/icon-48.png",
      "64": "images/icon-64.png"
    },
    "theme_icons": [
      {
        "light": "images/icon-dark-16.svg",
        "dark": "icons/icon-16.svg",
        "size": 16
      },
      {
        "light": "images/icon-dark-32.svg",
        "dark": "icons/icon-32.svg",
        "size": 32
      }
    ]
  },
  "icons": {
    "16": "images/icon-16.png",
    "24": "images/icon-24.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "64": "images/icon-64.png",
    "96": "images/icon-96.png",
    "128": "images/icon-128.png",
    "256": "images/icon-256.png"
  }
}